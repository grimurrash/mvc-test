{
    "require": {
        "klein/klein": "^2.1",
        "illuminate/database": "^9.20",
        "phpoption/phpoption": "^1.8",
        "vlucas/phpdotenv": "^5.4"
    },
    "config":{
        "optimize-autoloader":true
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Migrations\\": "database/migrations/"
        }
    }
}
